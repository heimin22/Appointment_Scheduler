<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#F8F9FA"
    tools:context=".MainActivity">


    <androidx.cardview.widget.CardView
        android:id="@+id/topPanel"
        android:layout_width="match_parent"
        android:layout_height="96dp"
        android:layout_gravity="center"
        android:layout_marginBottom="850dp"
        app:cardBackgroundColor="#BBDEFB"
        app:cardElevation="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingStart="55dp"
            android:paddingEnd="18dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/inter_bold"
                android:text="Your Schedules"
                android:textColor="#000000"
                android:textSize="25sp" />

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/calendarContainer"
        android:layout_width="372dp"
        android:layout_height="302dp"
        android:layout_gravity="center"
        android:layout_marginStart="19dp"
        android:layout_marginTop="137dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="452dp"
        app:cardBackgroundColor="#F8F9FA"
        app:cardCornerRadius="20dp"
        app:cardElevation="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/monthText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="June 2021"
            android:fontFamily="@font/inter_semibold"
            android:textColor="#1565C0"
            android:textSize="20dp"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="22dp"/>

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:src="@drawable/chevron_forward"
            android:layout_marginTop="23dp"
            android:layout_marginLeft="130dp"/>

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/chevron_backward"
            android:layout_marginLeft="280dp"
            android:layout_marginTop="23dp"/>

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/chevron_forward"
            android:layout_marginLeft="330dp"
            android:layout_marginTop="23dp"/>

        <GridLayout
            android:layout_width="311dp"
            android:layout_height="200dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:columnCount="7"
            android:rowCount="6">


            <TextView
                android:id="@+id/sunday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="0"
                android:paddingRight="20dp"
                android:textColor="#2196F3"
                android:textSize="13dp"
                android:fontFamily="@font/inter_regular"
                android:text="SUN" />

            <TextView
                android:id="@+id/monday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="1"
                android:paddingRight="20dp"
                android:textColor="#2196F3"
                android:textSize="13dp"
                android:fontFamily="@font/inter_regular"
                android:text="MON" />

            <TextView
                android:id="@+id/tuesday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="2"
                android:paddingRight="20dp"
                android:textColor="#2196F3"
                android:textSize="13dp"
                android:fontFamily="@font/inter_regular"
                android:text="TUE" />

            <TextView
                android:id="@+id/wednesday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="3"
                android:paddingRight="20dp"
                android:textColor="#2196F3"
                android:textSize="13dp"
                android:fontFamily="@font/inter_regular"
                android:text="WED" />

            <TextView
