<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#F8F9FA"
    tools:context=".MainActivity">


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.09" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.90" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.48" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5101172" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.64" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.66" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.79" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.08" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="845dp" />


    <androidx.cardview.widget.CardView
        android:id="@+id/topPanel"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_50sdp"
        android:layout_gravity="center"
        app:cardBackgroundColor="#BBDEFB"
        app:cardElevation="@dimen/_30sdp"
        app:layout_constraintBottom_toTopOf="@+id/guideLine9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:fontFamily="@font/inter_semibold"
            android:text="Your Schedules"
            android:textColor="#000000"
            android:textSize="20sp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/calendarContainer"
        android:layout_width="@dimen/_250sdp"
        android:layout_height="@dimen/_200sdp"
        android:layout_gravity="center"
        app:cardBackgroundColor="#F8F9FA"
        app:cardCornerRadius="20dp"
        app:cardElevation="30dp"
        app:layout_constraintBottom_toTopOf="@+id/guideLine4"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@+id/guideLine2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideLine1"
        app:layout_constraintTop_toTopOf="@+id/guideLine3"
        app:layout_constraintVertical_bias="0.12">


        <TextView
            android:id="@+id/monthText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="22dp"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/inter_semibold"
            android:text="June 2021"
            android:textColor="#1565C0"
            android:textSize="20dp" />

        <ImageButton
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginLeft="130dp"
            android:layout_marginTop="23dp"
            android:src="@drawable/chevron_forward" />

        <ImageButton
            android:id="@+id/prevMonthButton"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_12sdp"
            android:layout_marginLeft="280dp"
            android:layout_marginTop="23dp"
            android:src="@drawable/chevron_backward" />

        <ImageButton
            android:id="@+id/nextMonthButton"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_12sdp"
            android:layout_marginLeft="319dp"
            android:layout_marginTop="23dp"
            android:src="@drawable/chevron_forward" />

        <GridLayout
            android:id="@+id/calendarGrid"
            android:layout_width="311dp"
            android:layout_height="210dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:columnCount="7"
            android:rowCount="6">


            <TextView
                android:id="@+id/sunday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="0"
                android:fontFamily="@font/inter_semibold"
                android:paddingRight="20dp"
                android:text="SUN"
                android:textColor="#2196F3"
                android:textSize="13dp" />

            <TextView
                android:id="@+id/monday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="1"
                android:fontFamily="@font/inter_semibold"
                android:paddingRight="20dp"
                android:text="MON"
                android:textColor="#2196F3"
                android:textSize="13dp" />

            <TextView
                android:id="@+id/tuesday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="2"
                android:fontFamily="@font/inter_semibold"
                android:paddingRight="20dp"
                android:text="TUE"
                android:textColor="#2196F3"
                android:textSize="13dp" />

            <TextView
                android:id="@+id/wednesday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="3"
                android:fontFamily="@font/inter_semibold"
                android:paddingRight="20dp"
                android:text="WED"
                android:textColor="#2196F3"
                android:textSize="13dp" />

            <TextView
                android:id="@+id/thursday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="4"
                android:fontFamily="@font/inter_semibold"
                android:paddingRight="20dp"
                android:text="THU"
                android:textColor="#2196F3"
                android:textSize="13dp" />

            <TextView
                android:id="@+id/friday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="5"
                android:fontFamily="@font/inter_semibold"
                android:paddingRight="20dp"
                android:text="FRI"
                android:textColor="#2196F3"
                android:textSize="13dp" />

            <TextView
                android:id="@+id/saturday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="6"
                android:fontFamily="@font/inter_semibold    "
                android:paddingRight="20dp"
                android:text="SAT"
                android:textColor="#2196F3"
                android:textSize="13dp" />




        </GridLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/schedule1"
        android:layout_width="369dp"
        android:layout_height="120dp"
        android:layout_gravity="center"
        app:cardBackgroundColor="#F8F9FA"
        app:cardCornerRadius="20dp"
        app:cardElevation="30dp"
        app:layout_constraintBottom_toTopOf="@+id/guideLine6"
        app:layout_constraintEnd_toStartOf="@+id/guideLine2"
        app:layout_constraintStart_toStartOf="@+id/guideLine1"
        app:layout_constraintTop_toTopOf="@+id/guideline5">


        <TextView
            android:id="@+id/scheduleName1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="19dp"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/inter_regular"
            android:text="Name"
            android:textColor="#000000"
            android:textSize="20dp" />


        <TextView
            android:id="@+id/descriptionSample1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="48dp"
            android:fontFamily="@font/inter_light"
            android:text="Description:"
            android:textColor="#000000"
            android:textSize="12dp"
            tools:ignore="TextSizeCheck" />

        <TextView
            android:id="@+id/meetingLink"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="19dp"
            android:layout_marginTop="90dp"
            android:fontFamily="@font/inter_regular"
            android:text="Meeting Link:"
            android:textColor="#000000"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="290dp"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/inter_regular"
            android:text="00:00"
            android:textColor="#000000"
            android:textSize="20dp" />


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/schedule2"
        android:layout_width="369dp"
        android:layout_height="120dp"
        android:layout_gravity="center"
        app:cardBackgroundColor="#F8F9FA"
        app:cardCornerRadius="20dp"
        app:cardElevation="30dp"
        app:layout_constraintBottom_toTopOf="@+id/guideLine8"
        app:layout_constraintEnd_toStartOf="@+id/guideLine2"
        app:layout_constraintStart_toStartOf="@+id/guideLine1"
        app:layout_constraintTop_toTopOf="@+id/guideLine7">

        <TextView
            android:id="@+id/scheduleName2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="19dp"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/inter_regular"
            android:text="Name"
            android:textColor="#000000"
            android:textSize="20dp" />


        <TextView
            android:id="@+id/descriptionSample2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="48dp"
            android:fontFamily="@font/inter_light"
            android:text="Description:"
            android:textColor="#000000"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/meetingLink2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="19dp"
            android:layout_marginTop="90dp"
            android:fontFamily="@font/inter_regular"
            android:text="Meeting Link:"
            android:textColor="#000000"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/time2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="290dp"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/inter_regular"
            android:text="00:00"
            android:textColor="#000000"
            android:textSize="20dp" />


    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/navBar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_50sdp"
        android:translationY="@dimen/_6sdp"
        app:cardBackgroundColor="#F8F9FA"
        app:cardElevation="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideLine10">

        <ImageButton
            android:id="@+id/addButton"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginStart="@dimen/_105sdp"
            android:layout_marginTop="@dimen/_11sdp"
            android:backgroundTint="#F8F9FA"
            android:paddingBottom="10dp"
            android:src="@drawable/addv2" />

        <ImageButton
            android:id="@+id/homeButton"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginStart="@dimen/_30sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:backgroundTint="#F8F9FA"
            android:src="@drawable/homev2" />


        <TextView
            android:id="@+id/homeHint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_34sdp"
            android:layout_marginTop="@dimen/_37sdp"
            android:fontFamily="@font/inter_regular"
            android:text="Home"
            android:textColor="@color/black"
            android:textSize="@dimen/_8sdp" />

        <ImageButton
            android:id="@+id/profileButton"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginStart="@dimen/_180sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:backgroundTint="#F8F9FA"
            android:src="@drawable/profilev2" />

        <TextView
            android:id="@+id/profileHint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_183sdp"
            android:layout_marginTop="@dimen/_37sdp"
            android:fontFamily="@font/inter_regular"
            android:text="Profile"
            android:textColor="@color/black"
            android:textSize="@dimen/_8sdp" />

        <TextView
            android:id="@+id/addText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_37sdp"
            android:layout_marginStart="@dimen/_111sdp"
            android:fontFamily="@font/inter_regular"
            android:textColor="@color/black"
            android:text="Add"
            android:textSize="@dimen/_8sdp"/>

        <ImageButton
            android:id="@+id/menuButton"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:src="@drawable/menu"
            app:srcCompat="@drawable/menu"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_248sdp"
            android:background="#f8f9fa"/>

        <TextView
            android:id="@+id/menuText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_253sdp"
            android:layout_marginTop="@dimen/_37sdp"
            android:text="Menu"
            android:textColor="@color/black"
            android:textSize="@dimen/_8sdp" />


    </androidx.cardview.widget.CardView>

    <Button
        android:id="@+id/viewAllSched"
        android:background="@drawable/rounded_rectangle"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="wrap_content"
        android:text="View all schedules"
        android:textAllCaps="false"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/guideLine10"
        app:layout_constraintEnd_toStartOf="@+id/guideLine2"
        app:layout_constraintStart_toStartOf="@+id/guideLine1"
        app:layout_constraintTop_toTopOf="@+id/guideLine8" />


</androidx.constraintlayout.widget.ConstraintLayout>