                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="4"
                android:paddingRight="20dp"
                android:textColor="#2196F3"
                android:textSize="13dp"
                android:fontFamily="@font/inter_regular"
                android:text="THU" />

            <TextView
                android:id="@+id/friday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="5"
                android:paddingRight="20dp"
                android:textColor="#2196F3"
                android:textSize="13dp"
                android:fontFamily="@font/inter_regular"
                android:text="FRI" />

            <TextView
                android:id="@+id/saturday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="6"
                android:paddingRight="20dp"
                android:textColor="#2196F3"
                android:textSize="13dp"
                android:fontFamily="@font/inter_regular"
                android:text="SAT" />

            <TextView
                android:id="@+id/one"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:layout_row="1"
                android:layout_column="2"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="1" />

            <TextView
                android:id="@+id/two"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="3"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="2" />

            <TextView
                android:id="@+id/three"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="4"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:text="3" />

            <TextView
                android:id="@+id/four"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="5"
                android:paddingLeft="5dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="4" />

            <TextView
                android:id="@+id/five"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="6"
                android:paddingLeft="5dp"
                android:paddingTop="3dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="5" />

            <TextView
                android:id="@+id/six"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="0"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="6" />


            <TextView
                android:id="@+id/eight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="2"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="8" />

            <TextView
                android:id="@+id/nine"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="3"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="9" />

            <TextView
                android:id="@+id/ten"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="4"
                android:paddingLeft="5dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="10" />

            <ImageView
                android:id="@+id/seven"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_row="2"
                android:layout_column="1"
                android:paddingTop="7dp"
                android:paddingLeft="2dp"
                app:srcCompat="@drawable/seven" />

            <TextView
                android:id="@+id/eleven"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="5"
                android:paddingLeft="3dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="11" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="6"
                android:paddingLeft="1dp"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:textColor="#1565C0"
                android:fontFamily="@font/inter_regular"
                android:text="12" />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="3"
                android:layout_column="0"
                android:text="TextView" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="3"
                android:layout_column="1"
                android:text="TextView" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="3"
                android:layout_column="2"
                android:text="TextView" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="3"
                android:layout_column="3"
                android:text="TextView" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="3"
                android:layout_column="4"
                android:text="TextView" />


        </GridLayout>


    </androidx.cardview.widget.CardView>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="663dp"
        android:layout_marginTop="484dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/schedule1"
            android:layout_width="369dp"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            app:cardBackgroundColor="#F8F9FA"
            app:cardCornerRadius="30dp"
            app:cardElevation="30dp"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="486dp">


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/schedule2"
            android:layout_width="369dp"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:layout_marginBottom="20dp"
            app:cardBackgroundColor="#F8F9FA"
            app:cardCornerRadius="30dp"
            app:cardElevation="30dp"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="627dp">


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/schedule3"
            android:layout_width="369dp"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:layout_marginBottom="20dp"
            app:cardBackgroundColor="#F8F9FA"
            app:cardCornerRadius="30dp"
            app:cardElevation="30dp"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="770dp">

        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <androidx.cardview.widget.CardView
        android:id="@+id/navBar"
        android:layout_width="493dp"
        android:layout_height="124dp"
        android:layout_marginTop="841dp"
        app:cardBackgroundColor="#BBDEFB"
        app:cardCornerRadius="30dp"
        app:cardMaxElevation="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/addButton"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:backgroundTint="#BBDEFB"
            android:paddingBottom="10dp"
            android:src="@drawable/add" />

        <ImageButton
            android:id="@+id/homeButton"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="20dp"
            android:backgroundTint="#BBDEFB"
            android:src="@drawable/home" />

        <ImageButton
            android:id="@+id/profileButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/profile"
            android:backgroundTint="#BBDEFB"
            android:layout_marginLeft="345dp"
            android:layout_marginTop="32dp"/>



    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>